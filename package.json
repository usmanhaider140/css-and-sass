{
  "name": "css-and-sass",
  "version": "1.0.0",
  "main": "index.js",
  "dependencies": {
    "concat": "^1.0.3",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "node-sass": "^9.0.0",
    "postcss-cli": "^11.0.0",
    "sass": "^1.79.3"
  },
  "scripts": {
    "watch:sass": "node-sass sass/main.scss styles/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss styles/style.comp.css",
    "concat:css": "concat -o styles/style.concat.css styles/icon-fonts.css styles/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 100 versions' styles/style.concat.css -o styles/style.prefix.css",
    "compress:css": "node-sass styles/style.prefix.css styles/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
